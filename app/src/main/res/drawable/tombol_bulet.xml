<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/coklat"/>
            <stroke
                android:color="@color/putih"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid
                android:color="@color/colorPrimary"/>
            <stroke
                android:color="@color/abu_abu"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>

</selector>